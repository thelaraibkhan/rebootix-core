{
  "name": "@rebootix/zalo",
  "version": "2026.1.30",
  "description": "Rebootix Zalo channel plugin",
  "type": "module",
  "dependencies": {
    "rebootix": "workspace:*",
    "undici": "7.19.2"
  },
  "devDependencies": {
    "rebootix": "workspace:*"
  },
  "rebootix": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "zalo",
      "label": "Zalo",
      "selectionLabel": "Zalo (Bot API)",
      "docsPath": "/channels/zalo",
      "docsLabel": "zalo",
      "blurb": "Vietnam-focused messaging platform with Bot API.",
      "aliases": [
        "zl"
      ],
      "order": 80,
      "quickstartAllowFrom": true
    },
    "install": {
      "npmSpec": "@rebootix/zalo",
      "localPath": "extensions/zalo",
      "defaultChoice": "npm"
    }
  }
}
